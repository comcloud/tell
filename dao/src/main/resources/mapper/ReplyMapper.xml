<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yundingxi.tell.mapper.ReplyMapper">

    <select id="selectReplyById" resultType="com.yundingxi.tell.bean.entity.Reply">
        select *
        from reply
        where id=#{id}
    </select>
</mapper>